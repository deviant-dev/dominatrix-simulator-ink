+[minigame:success] -> finish_good
+[minigame:fail] -> finish_bad


= finish_good
/scene none
/scene Theatre.CunnilingusComplete
/music headmistress-reward-denouement
/ambient crowd
/character headmistress

~ embarrassed = true
