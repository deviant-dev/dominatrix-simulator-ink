/scene none
/scene Theatre.CunnilingusReady_Yuki
/music headmistress-reward-denouement
/character crowd cheers_loud
/character headmistress

+ [move:Approach] ->  load_yuki


//Load Full Minigame Scene
= load_hm
/scene none
/scene Theatre.Cunnilingus_HM
/music headmistress-reward
/ambient crowd
/character headmistress

// This minigame is on another character
~ mg_performOther = false

// Reset comment count
~ mg_commentCount = 0

// Skip 3 'continue' comments before saying another
~ mg_commentDelay = 4

//HM Sitting On Chair With Legs On Desk
